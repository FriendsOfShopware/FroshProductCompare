{% block frosh_product_compare_rating_cells %}
    {% for product in products %}
        {% block frosh_product_compare_rating_cell %}
            <td>
                {% sw_include '@Storefront/storefront/component/review/rating.html.twig' with {
                    points: product.ratingAverage,
                    style: 'text-primary'
                } %}
                <a class="product-detail-reviews-link"
                   target="_blank"
                   href="{{ path('frontend.detail.page', {'productId': product.id}) }}#review-tab-pane"
                   aria-controls="review-tab-pane">
                    {{ "froshProductCompare.section.content.reviewLinkText"|trans({'%count%': product.extensions.productReviews.reviewTotal})|sw_sanitize }}
                </a>
            </td>
        {% endblock %}
    {% endfor %}
{% endblock %}
